<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Tab id="departmentsTab" fx:id="departmentsTab" text="Departamente" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Departments.DepartmentsController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <ListView id="departmentsListView" fx:id="departmentsListView" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS" />
                        <VBox alignment="CENTER" prefHeight="37.0" prefWidth="600.0" spacing="30.0" VBox.vgrow="NEVER">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                            <children>
                                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Button id="addDepartmentButton" fx:id="addDepartmentButton" mnemonicParsing="false" text="Adauga" />
                                        <Button id="editDepartmentButton" fx:id="editDepartmentButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Editeaza" />
                                        <Button id="deleteDepartmentButton" fx:id="deleteDepartmentButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Sterge" />
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </content>
</Tab>