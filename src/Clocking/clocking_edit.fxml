<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Clocking.ClockingUIEdit">
   <children>
      <VBox alignment="CENTER" prefHeight="250.0" prefWidth="300.0">
         <children>
            <HBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0">
               <children>
                  <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label alignment="BASELINE_RIGHT" prefHeight="35.0" text="Data:  " />
                        <Label alignment="BASELINE_RIGHT" prefHeight="35.0" text="Intrare:  " />
                        <Label alignment="BASELINE_RIGHT" prefHeight="35.0" text="Inceput pauza:  " />
                        <Label alignment="BASELINE_RIGHT" prefHeight="35.0" text="Sfarsit pauza:  " />
                        <Label alignment="BASELINE_RIGHT" prefHeight="35.0" text="Iesire:  " />
                     </children>
                  </VBox>
                  <VBox prefHeight="280.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="dateField" promptText="zz.ll.aaaa" prefHeight="25.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="inField" prefHeight="25.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="breakField" prefHeight="25.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="workField" prefHeight="25.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="outField" prefHeight="25.0">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin></TextField>
                     </children></VBox>
               </children>
            </HBox>
            <Button fx:id="okButton" alignment="TOP_LEFT" mnemonicParsing="false" text="" />
         </children>
      </VBox>
   </children>
</AnchorPane>
